{
  "name": "tabnine-remix-demo",
  "version": "1.5.2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npm run clean && node ./server.mjs",
    "build": "npm run clean && remix vite:build",
    "build:icons": "tsx ./scripts/build-icons.ts",
    "start": "cross-env NODE_ENV=production node ./server.mjs",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true node server.mjs",
    "test": "vitest",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "coverage": "vitest run --coverage",
    "test:e2e": "npm run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "clean": "rimraf build",
    "nuke": "npm run clean && rimraf node_modules package-lock.json && npm i",
    "act": "doppler run -- act -s FLY_API_TOKEN --container-architecture linux/amd64",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@conform-to/zod": "^1.2.2",
    "@epic-web/config": "^1.16.3",
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@remix-run/express": "2.15.0",
    "@remix-run/node": "2.15.0",
    "@remix-run/react": "2.15.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "isbot": "^5.1.17",
    "lucide-react": "^0.462.0",
    "morgan": "^1.10.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "remix-auth": "^3.7.0",
    "remix-auth-form": "^1.5.0",
    "remix-typedjson": "^0.4.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.9",
    "@playwright/test": "^1.49.0",
    "@remix-run/dev": "2.15.0",
    "@remix-run/testing": "2.15.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.6",
    "all-contributors-cli": "^6.26.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "execa": "^9.5.1",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "jsdom": "^25.0.1",
    "node-html-parser": "^6.1.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^5.20.0",
    "remix-development-tools": "^4.7.7",
    "remix-flat-routes": "^0.6.5",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.19.2",
    "typescript": "5.7.2",
    "vite": "^5.4.9",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.6"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
